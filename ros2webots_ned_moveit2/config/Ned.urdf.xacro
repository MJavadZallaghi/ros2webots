<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="Ned">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import Ned urdf file -->
    <xacro:include filename="$(find ros2webots_manipulator)/resource/Ned.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="Ned.ros2_control.xacro" />


    <xacro:Ned_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
